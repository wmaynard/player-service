<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<RootNamespace>PlayerService</RootNamespace>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="adjectives.txt" />
	  <Content Include="adjectives.txt">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <None Remove="nouns.txt" />
	  <Content Include="nouns.txt">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <None Remove="GeoIP2-Country.mmdb" />
	  <Content Include="GeoIP2-Country.mmdb">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	  <None Remove="geo-ip.mmdb" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Google.Apis.Auth.AspNetCore3" Version="1.55.0" />
	  <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="6.15.0" />
	  <PackageReference Include="platform-csharp-common" Version="1.0.96" />
	</ItemGroup>

</Project>
