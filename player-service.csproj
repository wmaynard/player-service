<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<RootNamespace>PlayerService</RootNamespace>
		<LangVersion>10</LangVersion>
		<AssemblyVersion>2.0.*</AssemblyVersion>
		<Deterministic>false</Deterministic>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="adjectives.txt" />
	  <Content Include="adjectives.txt">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <None Remove="nouns.txt" />
	  <Content Include="nouns.txt">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <None Remove="GeoIP2-Country.mmdb" />
	  <Content Include="GeoIP2-Country.mmdb">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	  <None Remove="geo-ip.mmdb" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Google.Apis.Auth.AspNetCore3" Version="1.55.0" />
	  <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="6.15.0" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\platform-csharp-common\platform-csharp-common.csproj" />
	</ItemGroup>

</Project>
